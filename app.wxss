/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
text{
  color: white
}
.model-gift{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background:rgba(16, 33, 63, 0.6);
}
.model-gift-box{
  width: 560rpx;
  height: 650rpx;
  margin: 200rpx auto 0;
  background:url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  padding-top:40rpx;
}
.golds-image{
  width: 292rpx;
  height: 197rpx;
  display: block;
  margin: 0 auto;
  margin-bottom:40rpx;
  margin-top:96rpx;

}
.hint-box{
  width: 480rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #999;
  line-height: 50rpx;
  margin:30rpx auto 0;
}
.get-gift-btn{
  margin-top: 50rpx;
  width: 480rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 45rpx;
  text-align: center;
  color: #fff;
  background:linear-gradient(to right,#FB5847, #7802F3);
}
