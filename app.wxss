/**app.wxss**/
.container {
   /* height: 100%;  */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
text{
  color: white
}
.model-gift{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background:rgba(16, 33, 63, 0.6);
}
.model-fail{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background:rgba(17, 10, 28, 0.9);
}
.model-gift-box{
  width: 560rpx;
  height: 650rpx;
  margin: 200rpx auto 0;
  background:url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  padding-top:40rpx;
}
.model-fail-box{
  width: 560rpx;
  height: 650rpx;
  margin: 200rpx auto 0;
  background: #FFF;
  border-radius: 20rpx;
}
.golds-image{
  width: 292rpx;
  height: 197rpx;
  display: block;
  margin: 0 auto;
  margin-bottom:40rpx;
  margin-top:96rpx;

}

 .sad-image{
  width: 200rpx;
  height: 187rpx;
  display: block;
  margin: 0 auto;
} 

.hint-box{
  width: 480rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #999;
  line-height: 50rpx;
  margin:30rpx auto 0;
}
.hint-box-fail{
  width: 480rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 50rpx;
  margin:30rpx auto 0;
  color: rgb(149, 148, 153)
}
.get-gift-btn{
  margin-top: 50rpx;
  width: 480rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 45rpx;
  text-align: center;
  color: #fff;
  font-size: 29rpx;
  background:linear-gradient(to right,#FA5846, #FA1471);
}
.btn_1, .btn_2{
   margin-top: 30rpx; 
}
.btn_1{
  background:linear-gradient(to right,#7D94FE, #53CDFF);
}
.btn_2{
  background:linear-gradient(to right,#FC554C, #FF0978);
}
.notEnough{
  width: 100%;
  height: 70rpx;
  text-align: center;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  font-weight: bolder;
  letter-spacing: 2rpx;
  font-size: 35rpx;
}
.back{
  position: fixed;
  right: 20rpx;
  top:20rpx;
  width: 80rpx;
  height: 80rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%
}.rank_{
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 2rpx;
  font-size: 48rpx;
}
.jiangbei{
  width: 50rpx;
  height: 60rpx;
  margin-right: 20rpx;
}.user_{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.userinfo-avatar {
  width: 100rpx;
  height: 90rpx;
  margin: 20rpx;
  border-radius: 50%;
}
/* 超出三个字已省略号隐藏 */
.userinfo-nickname {
  width: 100rpx;
  color: black;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.my_rank{
  height: 186rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
  background: url(data:image/png;base64,)repeat-x;
  background-size: 70% 100%;
}
.friend_rank{
  height: 186rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
}
.my_rank_left{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center
}
.rank_num{
  color: rgb(230, 48, 48)
}
.money{
  color: rgb(230, 48, 48)
}
.send_pk{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 50rpx;
  border: transparent;
  background: rgba(255, 255, 255, 0.5)
}
.btn_send{
  background: transparent
}
.img_send{
  width: 500rpx;
  height: 100rpx;
  display: block;
  margin: 0 auto;
}
.rank_123st{
  width: 48rpx;
  height: 65rpx;
}
page{
  padding-bottom: 200rpx;
}