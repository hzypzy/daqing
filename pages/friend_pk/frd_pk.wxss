.rank_{
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 2rpx;
  font-size: 48rpx;
}
.jiangbei{
  width: 50rpx;
  height: 60rpx;
  margin-right: 20rpx;
}.user_{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.userinfo-avatar {
  width: 100rpx;
  height: 90rpx;
  margin: 20rpx;
  border-radius: 50%;
}
/* 超出三个字已省略号隐藏 */
.userinfo-nickname {
  width: 100rpx;
  color: black;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.my_rank{
  height: 186rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
  background: url(data:image/png;base64,)repeat-x;
  background-size: 70% 100%;
}
.friend_rank{
  height: 186rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
}
.my_rank_left{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center
}
.rank_num{
  color: rgb(230, 48, 48)
}
.money{
  color: rgb(230, 48, 48)
}
.send_pk{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 50rpx;
  background: rgba(255, 255, 255, 0.5)
}
.img_send{
  width: 500rpx;
  height: 100rpx;
  display: block;
  margin: 0 auto;
}
.rank_123st{
  width: 48rpx;
  height: 65rpx;
}
page{
  padding-bottom: 200rpx;
}