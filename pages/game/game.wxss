
page{
  background: rgba(77, 132, 232, 1)
}

.top{
  width: 100%;
  height: 90rpx;
  background: linear-gradient(rgba(115, 200, 250, 1), rgba(55, 147, 240, 1));
  border-top: 3rpx solid rgba(68, 119, 232, 1);
  border-bottom: 3rpx solid rgba(95, 166, 243, 1);
}
.top_border{
  width: 100%;
  height: 84rpx;
  border-top:3rpx solid rgba(134, 250, 254, 1);
  border-bottom: 3rpx solid rgba(48, 131, 241, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around
}
.tm{
  width: 155rpx;
  height: 60rpx;
  border-radius: 50rpx;
  background: rgba(39, 100, 191, 1);
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center
}
.mid{
  width: 100%;
  height: 225rpx;
  display: flex;
  justify-content: center;
  align-items: center
}
.circle{
  width: 144rpx;
  height: 144rpx;
  background: url()no-repeat;
}