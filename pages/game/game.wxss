
page{
  background: rgba(77, 132, 232, 1)
}

.top{
  width: 100%;
  height: 90rpx;
  background: linear-gradient(rgba(115, 200, 250, 1), rgba(55, 147, 240, 1));
  border-top: 3rpx solid rgba(68, 119, 232, 1);
  border-bottom: 3rpx solid rgba(95, 166, 243, 1);
}
.top_border{
  width: 100%;
  height: 84rpx;
  border-top:3rpx solid rgba(134, 250, 254, 1);
  border-bottom: 3rpx solid rgba(48, 131, 241, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around
}
.tm{
  width: 155rpx;
  height: 60rpx;
  border-radius: 50rpx;
  background: rgba(39, 100, 191, 1);
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center
}
.mid{
  width: 100%;
  height: 225rpx;
  display: flex;
  justify-content: center;
  align-items: center
}
.circle{
  width: 144rpx;
  height: 144rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  border: 6rpx solid white;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center
}
.circle_lit{
  width: 100rpx;
  height: 100rpx;
  border:6rpx solid white;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center
}
.circle_lit text{
  font-size: 30rpx;
}
.music{
  width: 45rpx;
  height: 45rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  position: absolute;
  right: 20rpx;
}
.chengyu{
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 118rpx;
  color: black;

  font-size: 125rpx;

  letter-spacing: 5rpx;
}
.share{
  width: 100%;
  height: 118rpx;
  display: flex;
  justify-content: center;
  align-items: center
}
.share_frd, .share_tem{
  width: 200rpx;
  height: 50rpx;
  background: url(data:image/png;base64,)no-repeat 0 0;
  background-size: 200% 100%;
}
.share_tem{
  background-position-x: -200rpx;
}
.z{
  width: 100%;
  height: 380rpx;
  display: flex;
  justify-content: space-around;
  align-items: center
}
.z_1, .z_2{
  width: 300rpx;
  height: 300rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size: 100% 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 125rpx;
  color: rgba(55, 75, 116, 1)
}
.xv_1, .xv_2{
  width: 150rpx;
  height: 150rpx;
  position: absolute;
}
.xv_1{
  left: 116rpx;
  bottom: 176rpx;
}
.xv_2{
  right: 116rpx;
  bottom: 176rpx;
}